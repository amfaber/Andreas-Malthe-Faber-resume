    "#https://open.kattis.com/problems/character\n",
    "#COMPLETED\n",
    "import math\n",
    "inp = int(input())\n",
    "out = 0\n",
    "def C(N, k):\n",
    "    return math.factorial(N)//math.factorial(k)//math.factorial(N-k)\n",
    "for i in range(2, inp+1):\n",
    "    out += C(inp, i)\n",
    "print(out)"
   ]
  }
