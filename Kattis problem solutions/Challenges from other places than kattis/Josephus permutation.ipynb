    "#https://edabit.com/challenge/4AjWvJdZpFEMbGALd\n",
    "#COMPLETED\n",
    "def who_goes_free(P, k):\n",
    "    l = [i for i in range(P)]\n",
    "    i = -1\n",
    "    while len(l) > 1:\n",
    "        i = (i + k) % len(l)\n",
    "        l.pop(i)\n",
    "        i -= 1\n",
    "    return l[0]\n"
   ]
